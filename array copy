#include<stdio.h>
int main()
{
    int a[5]={1,2,3,4,5};//array a is decalared and initialized with five elements
    int *p[5];//pointer array p is decalared with size five
    printf("original array elements:");
    for(int i=0;i<5;i++)
    {
        //printf("%d \t",a[i]); //it prints the array elements by using array indexes
        printf("%d \t",*(a+i)); //it prints the array elements by using array modifier
    }
    printf("\n duplicate array elements:");
    for(int i=0;i<5;i++)
    {
        p[i]=&a[i];
        //printf("  %d \t",*(p[i])); //it prints the array elements by using the pointer array indexes
        printf(" %d \t",**(p+i)); //it prints the array elements by using the pointer array modifier
    }
}

output:
original array elements:1       2       3       4       5 
duplicate array elements: 1     2       3       4       5 
