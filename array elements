#include<stdio.h>
int main()
{
    int a[5]={1,2,3,4,5};
    int *p[5];
    for(int i=0;i<5;i++)
    {
        p[i]=&a[i];
        //printf("%d \n",a[i]); //it prints the array elements by using array indexes
        //printf("%p \n",(a+i)); //it prints the address of a array elements by using array modifier
        //printf("%d \n",*(a+i)); //it prints the array elements by using array modifier
        //printf("%d \n",*p[i]); //it prints the array elements by using the pointer array indexes
        printf("%d \n",**(p+i)); //it prints the array elements by using the pointer array modifier
    }
}

output:

1
2
3
4
5
