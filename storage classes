#include<stdio.h>
void fun()
{
    static int b=5;
    ++b;
    printf("static=%d\n",b);
    auto int a=10;
    ++a;
    printf("auto=%d \n",a);
    register int c=5;
    ++c;
    printf("register=%d \n",c);
}
int main()
{
    fun();
    fun();
    fun();
}

output:

static=6
auto=11 
register=6
static=7
auto=11 
register=6
static=8
auto=11 
register=6
