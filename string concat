#include<stdio.h>
#include<string.h>
int main()
{
    char s1[10];
    char s2[10];
    printf("enter first string: \n");
    scanf("%s",s1);
    printf("enter second string: \n");
    scanf("%s",s2);
    strcat(s1,s2);
    printf("string concatenation:%s",s1);
}

output:

enter first string:
priya
enter second string:
pinky
string concatenation:priyapinky
    
