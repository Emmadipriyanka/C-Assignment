#include<stdio.h>
#include<string.h>
int main()
{
    char s1[10];
    char s2[10];
    printf("enter first string: \n");
    scanf("%s",s1);
    printf("enter second string: \n");
    scanf("%s",s2);
    strcat(s1,s2);
    printf("string concatenation:%s",s1);
}

output:

enter first string:
priya
enter second string:
pinky
string concatenation:priyapinky
    


#include <stdio.h>
int main()
{
    char str1[100], str2[100],str3[100];
    int i = 0, j = 0;
    printf("enter First string:%s \n", str1);
    scanf("%s",str1);
    printf("enter Second string:%s \n", str2);
    scanf("%s",str2);
    while (str1[i] != '\0') {
        str3[j] = str1[i];
        i++;
        j++;
    }
    i = 0;
    while (str2[i] != '\0') {
        str3[j] = str2[i];
        i++;
        j++;
    }
    str3[j] = '\0';
    printf("\n string Concatenation: %s", str3);
    len_string(str3);
    return 0;
}
  
output:

enter first string:
priya
enter second string:
pinky
string concatenation:priyapinky
    
